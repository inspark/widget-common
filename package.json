{
  "name": "widget-common-container",
  "version": "0.0.0",
  "scripts": {
    "clean": "rm -rf dist/*",
    "build_full": "copy-files-from-to --config ./projects/widget-common/versions/full/copy.json && ng build",
    "build_interface": "copy-files-from-to --config ./projects/widget-common/versions/interfaces/copy.json && ng build",
    "build": "npm run clean && npm run build_full && npm run build_interface && copy-files-from-to --config ./projects/widget-common/copy.json --mode production && node update_full.js",
    "dev": "ng build --watch=true",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "publish": "npm publish dist/widget-common/"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^9.1.13",
    "@angular/common": "^9.1.13",
    "@angular/compiler": "^9.1.13",
    "@angular/core": "^9.1.13",
    "@angular/forms": "^9.1.13",
    "@angular/platform-browser": "^9.1.13",
    "@angular/platform-browser-dynamic": "^9.1.13",
    "@angular/router": "^9.1.13",
    "@types/forge-viewer": "^7.53.0",
    "core-js": "^2.5.4",
    "echarts": "^5.2.2",
    "link": "^0.1.5",
    "ng2-cookies": "^1.0.12",
    "ngx-echarts": "^5.2.2",
    "node-sass": "^4.14.1",
    "resize-observer-polyfill": "^1.5.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.15",
    "@angular-devkit/build-ng-packagr": "~0.901.15",
    "@angular/cli": "^15.1.4",
    "@angular/compiler-cli": "^9.1.13",
    "@angular/http": "^7.0.4",
    "@angular/language-service": "^9.1.13",
    "@ng-bootstrap/ng-bootstrap": "^4.1.3",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^3.0.1",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^8.10.48",
    "codelyzer": "~4.5.0",
    "copy-files-from-to": "^2.0.2",
    "d3": "^3.5.15",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "moment": "^2.24.0",
    "ng-inline-svg": "^8.4.1",
    "ng-packagr": "^9.1.5",
    "ng2-nvd3": "^2.0.0",
    "protractor": "~5.4.0",
    "rxjs": "^6.3.3",
    "ts-node": "^9.1.1",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.8.3"
  }
}
